<?xml version="1.0" encoding="UTF-8"?>
<configuration status="WARN">
  <appenders>
    <File name="CMPE_LOG" fileName="logs/cmpe.log">
    	<PatternLayout pattern="%d{yyyy-mm-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </File>             
  </appenders>

  <loggers>     
    <root level="debug">
       <appender-ref ref="CMPE_LOG" level="error"/>
    </root>    
  </loggers>
</configuration>